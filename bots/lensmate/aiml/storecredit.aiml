<?xml version="1.0" encoding="ISO-8859-1"?>
<aiml version="2.0">
	<category>
		<pattern>STORECREDIT</pattern>
		<template>
            Enter you email to get <set name = "topic">storecredit</set> info.
        </template>
	</category>
	<category>
		<pattern>WALLET</pattern>
		<template>
            <srai>STORECREDIT</srai>
        </template>
	</category>
	<category>
		<pattern>STORECREDIT *</pattern>

            <srai>STORECREDIT</srai>
        </template>
	</category>
	<category>
		<pattern>AVAILABLE STORECREDIT *</pattern>
		<template>
            <srai>STORECREDIT</srai>
        </template>
	</category>
	<category>
		<pattern>AVAILABLE BALANCE *</pattern>
		<template>
            <srai>STORECREDIT</srai>
        </template>
	</category>
	<category>
		<pattern>WALLET BALANCE</pattern>
		<template>
            <srai>STORECREDIT</srai>
        </template>
	</category>
	<category>
		<pattern>AVAILABLE WALLET BALANCE</pattern>
		<template>
            <srai>STORECREDIT</srai>
        </template>
	</category>
	<topic name = "storecredit">
		<category>
		 	<pattern>*</pattern>
		 	<template><fetch method="storeCredit"><star/></fetch><think><set name = "topic">greetings</set></think></template>
	 	</category>
	</topic>
</aiml>